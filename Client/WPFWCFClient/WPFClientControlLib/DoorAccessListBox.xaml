<UserControl  x:Class="WPFClientControlLib.DoorAccessListBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFClientControlLib"
        mc:Ignorable="d"
       d:DesignHeight="300"  d:DesignWidth="300">
    <Grid>
        <DataGrid Name="DataGrid1" AutoGenerateColumns="False" Margin="0,-26,0,0" >
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding DevInfo.Name}" Header="Name"></DataGridTextColumn>
                <DataGridTextColumn Binding="{Binding DevInfo.Path}" Header="Path"></DataGridTextColumn>
                <DataGridTextColumn Binding="{Binding DevInfo.TypeCode}" Header="TypeCode"></DataGridTextColumn>
                <DataGridTextColumn Binding="{Binding DevInfo.ModelName}" Header="ModelName"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
