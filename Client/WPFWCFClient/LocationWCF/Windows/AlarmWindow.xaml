<Window x:Class="LocationWCFClient.Windows.AlarmWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocationWCFClient.Windows"
        xmlns:wpfClientControlLib="clr-namespace:WPFClientControlLib;assembly=WPFClientControlLib"
        mc:Ignorable="d"
        Title="AlarmWindow" Height="300" Width="300" Loaded="AlarmWindow_OnLoaded">
    <Grid>
        <TabControl>
            <TabItem Header="设备告警">
                <DataGrid Name="DataGridDeviceAlarms"></DataGrid>
            </TabItem>
            <TabItem Header="定位告警">
                <DataGrid Name="DataGridLocationAlarms"></DataGrid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
