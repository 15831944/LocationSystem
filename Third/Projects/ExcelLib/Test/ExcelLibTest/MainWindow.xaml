<Window x:Class="ExcelLibTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelLibTest"
        mc:Ignorable="d"
        Title="打开表格" Height="350" Width="525" Loaded="MainWindow_OnLoaded">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="File">
                <MenuItem Header="Open" Name="MenuOpenFile" Click="MenuOpenFile_OnClick"></MenuItem>
            </MenuItem>
        </Menu>
        <DataGrid Name="DataGrid1"></DataGrid>
    </DockPanel>
</Window>
