<Window x:Class="ArchorUDPTool.CodeTestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArchorUDPTool"
        mc:Ignorable="d"
        Title="CodeTestWindow" Height="450" Width="1200">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="转换" Name="MenuChange" Click="MenuChange_Click"></MenuItem>
        </Menu>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"></ColumnDefinition>
                <ColumnDefinition Width="2*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <TextBox Name="Tb1" TextWrapping="Wrap" AcceptsTab="True"></TextBox>
            <TextBox Grid.Column="1" Name="Tb2" TextWrapping="Wrap"></TextBox>
        </Grid>
    </DockPanel>
    
</Window>
