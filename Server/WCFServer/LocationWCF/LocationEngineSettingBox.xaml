<UserControl x:Class="LocationServer.LocationEngineSettingBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LocationServer"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top">
            <Button Name="BtnModifyArchor1FromArchor2" Click="BtnModifyArchor1FromArchor2_OnClick">根据基站列表2修改基站列表1</Button>
            <Button Name="BtnBindingArchor" Click="BtnBindingArchor_OnClick">基站绑定</Button>
        </StackPanel>
        <TabControl>
            <TabItem Header="基站列表1">
                <DataGrid Name="DataGridArchor1">

                </DataGrid>
            </TabItem>
            <TabItem Header="基站列表2">
                <DataGrid Name="DataGridArchor2">

                </DataGrid>
            </TabItem>
            <TabItem Header="标签列表">
                <DataGrid Name="DataGrid2">

                </DataGrid>
            </TabItem>
        </TabControl>
    </DockPanel>
    
</UserControl>
