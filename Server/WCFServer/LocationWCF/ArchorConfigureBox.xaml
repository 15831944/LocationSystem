<UserControl x:Class="LocationServer.ArchorConfigureBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LocationServer"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="600">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal">
            <Label>Remote IP</Label>
            <TextBox Width="100" Name="TbRemoteIp" Text="192.168.10.255"></TextBox>
            <Label>Port</Label>
            <TextBox Width="100" Name="TbPort" Text="4646"></TextBox>
            <Button Name="BtnSearch" Click="BtnSearch_OnClick">搜索</Button>
        </StackPanel>
        <DataGrid Name="DataGrid3"></DataGrid>
    </DockPanel>
</UserControl>
