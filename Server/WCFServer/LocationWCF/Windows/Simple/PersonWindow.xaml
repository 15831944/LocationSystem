<Window x:Class="LocationServer.Windows.PersonWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocationServer.Windows"
        mc:Ignorable="d"
        Title="PersonWindow" Height="400" Width="600" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <DockPanel>
        <StackPanel DockPanel.Dock="Bottom" Name="ToolBar1" Visibility="Collapsed">
            <Button Name="BtnOk" Click="BtnOk_OnClick">确定</Button>
        </StackPanel>
        <DataGrid Name="DataGrid1">
            <DataGrid.ContextMenu>
                <ContextMenu>
                    <MenuItem Header="发卡" Name="MenuBindTag" Click="MenuBindTag_Click"></MenuItem>
                </ContextMenu>
            </DataGrid.ContextMenu>
        </DataGrid>
    </DockPanel>
</Window>
