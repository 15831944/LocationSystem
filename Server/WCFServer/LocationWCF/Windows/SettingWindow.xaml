<Window x:Class="LocationServer.Windows.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocationServer.Windows"
        mc:Ignorable="d"
        Title="选项" Height="200" Width="400" Loaded="SettingWindow_OnLoaded">
    <TabControl>
        <TabItem Header="常规">
            <DockPanel>
                <StackPanel DockPanel.Dock="Bottom">
                    <Button Name="BtnSave" Click="BtnSave_OnClick">保存</Button>
                </StackPanel>
                <StackPanel>
                    <CheckBox Content="开机自启动" Name="CbAutoRun"></CheckBox>
                    <!--<CheckBox Content="自动启动服务" Name="CbAutoStartServer"></CheckBox>
                <CheckBox Content="自动连接定位引擎" Name="CbAutoConnectLocationEngine"></CheckBox>-->
                </StackPanel>
            </DockPanel>
            
        </TabItem>
    </TabControl>
</Window>
