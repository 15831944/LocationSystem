<Window x:Class="LocationServer.Windows.InitNewContextWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocationServer.Windows"
        mc:Ignorable="d"
        Title="InitNewContextWindow" Height="450" Width="800">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="CreateInitInfo"></MenuItem>
        </Menu>
        <TabControl>
            <TabItem Header="Setting">
                <StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Label Content="名称：" Width="100" HorizontalContentAlignment="Right"></Label>
                        <TextBox x:Name="TbParkName" Width="200"></TextBox>
                    </StackPanel>
                </StackPanel>
            </TabItem>
            <TabItem Header="TabInitInfo">
                <TextBox Name="TextBoxInitInfoXml"></TextBox>
            </TabItem>
            <TabItem Header="Log">
                <TextBox x:Name="TbLog"></TextBox>
            </TabItem>
        </TabControl>
    </DockPanel>
</Window>
